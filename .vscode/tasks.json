{
  "tasks": [
    {
      "type": "shell",
      "label": "cl.exe build active file",
      "command": "cl.exe",
      "args": [
        "/Zi",
        "/EHsc",
        "/MD",
        "/Fe:",
        "${workspaceFolder}\\program.exe",
        "src\\program.cpp",
        "glfw3.lib",
        "user32.lib",
        "kernel32.lib",
        "shell32.lib",
        "gdi32.lib",
        "glew32s.lib",
        "opengl32.lib"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ],
  "version": "2.0.0"
}
